{
  "name": "aristath/kirki",
  "type": "library",
  "keywords": ["WordPress", "Customizer"],
  "description": "Extending the WordPress customizer",
  "homepage": "https://kirki.org",
  "license": "MIT",
  "authors": [
    {
      "name": "aristath",
      "email": "aristath@gmail.com",
      "homepage": "https://aristath.github.io",
